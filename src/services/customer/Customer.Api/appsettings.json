{
  "MongoConnection": {
    "ConnectionString": "mongodb://localhost:27017"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://microexample-elk:9200",
          "indexFormat": "microexample-customer-service-{0:yyyy.MM}",
          "templateName": "microexampleTemplate",
          "minimumLogEventLevel": "Information"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information"
    }
  }
}
